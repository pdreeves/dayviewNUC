	"dataSources": {
		"ds_RcJFjz3f": {
			"type": "ds.search",
			"options": {
				"query": "index=network source=\"netmon-pi-dayview\" mtrTarget=8.8.8.8 OR mtrTarget=208.67.222.222 OR mtrTarget=13.33.25.126"
			},
			"name": "baseLatency"
		},
		"ds_nCJ8uBUp": {
			"type": "ds.chain",
			"options": {
				"query": "|  search mtrTarget=$dd_ICRMfoZl$\n| stats latest(mtrAvg)",
				"extend": "ds_RcJFjz3f"
			},
			"name": "mtrAvgLatest"
		},
		"ds_cEXvDuCN": {
			"type": "ds.chain",
			"options": {
				"query": "|  search mtrTarget=$dd_ICRMfoZl$\n| timechart avg(mtrAvg) by mtrTarget",
				"extend": "ds_RcJFjz3f"
			},
			"name": "mtrAvgTimechart"
		},
		"ds_jvAcHNkh": {
			"type": "ds.search",
			"options": {
				"query": "index=network source=\"netmon-pi-dayview\" sourcetype=\"netmon-pi-speedtest\""
			},
			"name": "baseSpeedtest"
		},
		"ds_UDcUEjCZ": {
			"type": "ds.search",
			"options": {
				"query": "index=network host=dayviewPi source=\"netmon-pi-dayview\" sourcetype=\"netmon-pi-speedtest\"\n| spath input=line"
			},
			"name": "baseThroughput"
		},
		"ds_iS2SOpny": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_UDcUEjCZ",
				"query": "| stats latest(download.bandwidth) as download\n| eval download = 8 * download/1000/1000"
			},
			"name": "latestDownload"
		},
		"ds_QyaHXJ66": {
			"type": "ds.chain",
			"options": {
				"query": "| stats latest(upload.bandwidth) as upload\n| eval upload = 8 * upload/1000/1000",
				"extend": "ds_UDcUEjCZ"
			},
			"name": "latestUpload"
		},
		"ds_CeGSHOQ8": {
			"type": "ds.chain",
			"options": {
				"query": "| timechart values(upload.bandwidth) as upload, values(download.bandwidth) as download\n| eval upload = 8 * upload/1000/1000, download = 8 * download/1000/1000",
				"extend": "ds_UDcUEjCZ"
			},
			"name": "timechartThroughput"
		}
	},
	"defaults": {
		"dataSources": {
			"ds.search": {
				"options": {
					"queryParameters": {
						"latest": "$global_time.latest$",
