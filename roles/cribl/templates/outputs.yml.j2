outputs:
  splunkDayview:
    systemFields:
      - cribl_pipe
    port: 9998
    nestedFields: none
    throttleRatePerSec: "0"
    tls:
      disabled: true
    connectionTimeout: 10000
    writeTimeout: 60000
    enableMultiMetrics: false
    enableACK: true
    onBackpressure: queue
    pqMaxFileSize: 20 MB
    pqPath: $CRIBL_HOME/state/queues
    pqCompress: none
    authType: manual
    authToken: ""
    type: splunk
    host: splunk.dayview.greentangent.net